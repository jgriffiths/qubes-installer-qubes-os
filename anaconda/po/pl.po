msgid "/boot file system"
msgstr "System plików /boot"

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:383
#, python-format
msgid ""
"RAID sets that contain '%(desc)s' must have one of the following raid "
"levels: %(raid_level)s."
msgstr ""
"Zestawy RAID zawierające „%(desc)s” muszą być jednego z następujących "
"poziomów RAID: %(raid_level)s."

# translation auto-copied from project anaconda, version rhel7-branch,
# document anaconda, author Piotr Drąg
#: pyanaconda/bootloader.py:391
#, python-format
msgid ""
"RAID sets that contain '%(desc)s' must have one of the following metadata "
"versions: %(metadata_versions)s."
msgstr ""
"Zestawy RAID zawierające „%(desc)s” muszą posiadać jedną z poniższych wersji"
" metadanych: %(metadata_versions)s"

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:399
#, python-format
msgid ""
"RAID sets that contain '%(desc)s' must have one of the following device "
"types: %(types)s."
msgstr ""
"Zestawy RAID zawierające „%(desc)s” muszą posiadać jeden z poniższych typów "
"urządzeń: %(types)s."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:415
#, python-format
msgid "%(name)s must have one of the following disklabel types: %(types)s."
msgstr ""
"%(name)s musi posiadać jeden z poniższych typów etykiet dysków: %(types)s."

# translation auto-copied from project anaconda, version rhel7-branch,
# document anaconda, author Piotr Drąg
#: pyanaconda/bootloader.py:427
#, python-format
msgid "%(desc)s cannot be of type %(type)s."
msgstr "%(desc)s nie może być typu %(type)s."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:433
#, python-format
msgid "%(desc)s must be mounted on one of %(mountpoints)s."
msgstr "%(desc)s musi być zamontowane na jednym z %(mountpoints)s."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:445
#, python-format
msgid "%(desc)s must be between %(min)d and %(max)d MB in size"
msgstr "%(desc)s musi mieć rozmiar między %(min)d a %(max)d MB"

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:451
#, python-format
msgid "%(desc)s must not be smaller than %(min)dMB."
msgstr "%(desc)s nie może być mniejsze niż %(min)d MB."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:460
#, python-format
msgid "%(desc)s must not be larger than %(max)dMB."
msgstr "%(desc)s nie może być większe niż %(max)d MB."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:478
#, python-format
msgid "%(desc)s must be within the first %(max_end)s of the disk."
msgstr "%(desc)s musi znajdować się w pierwszych %(max_end)s dysku."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:488
#, python-format
msgid "%s must be on a primary partition."
msgstr "%s musi znajdować się na partycji podstawowej."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:624 pyanaconda/bootloader.py:724
#, python-format
msgid "%s cannot be on an encrypted block device."
msgstr "%s nie może znajdować się na zaszyfrowanym urządzeniu blokowym."

# translation auto-copied from project anaconda, version rhel7-branch,
# document anaconda, author Piotr Drąg
#: pyanaconda/bootloader.py:679
#, python-format
msgid "%s cannot be on an iSCSI disk"
msgstr "%s nie może znajdować się na dysku iSCSI"

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:683
#, python-format
msgid "%(desc)s cannot be of type %(type)s"
msgstr "%(desc)s nie może być typu %(type)s"

# translation auto-copied from project anaconda, version rhel7-branch,
# document anaconda, author Piotr Drąg
#: pyanaconda/bootloader.py:1365
#, python-format
msgid ""
"boot loader stage2 device %(stage2dev)s is on a multi-disk array, but boot "
"loader stage1 device %(stage1dev)s is not. A drive failure in %(stage2dev)s "
"could render the system unbootable."
msgstr ""
"urządzenie stage2 %(stage2dev)s programu startowego jest na macierzy "
"wielodyskowej, ale urządzenie stage1 %(stage1dev)s programu startowego nie "
"jest. Awaria napędu w %(stage2dev)s może spowodować niemożliwość "
"uruchomienia systemu."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:1371
#, python-format
msgid ""
"boot loader stage2 device %(stage2dev)s is on a multi-disk array, but boot "
"loader stage1 device %(stage1dev)s is not part of this array. The stage1 "
"boot loader will only be installed to a single drive."
msgstr ""
"urządzenie stage2 %(stage2dev)s programu startowego jest na macierzy "
"wielodyskowej, ale urządzenie stage1 %(stage1dev)s programu startowego nie "
"jest częścią tej macierzy. Program startowy stage1 zostanie zainstalowany "
"tylko na jednym napędzie."

# translation auto-copied from project anaconda, version f22-branch, document
# anaconda
#: pyanaconda/bootloader.py:1666
#, python-format
msgid ""
"%(deviceName)s may not have enough space for grub2 to embed core.img when "
"using the %(fsType)s file system on %(deviceType)s"
msgstr ""
"%(deviceName)s może nie posiadać wystarczającej ilości miejsca, aby GRUB2 "
"mógł osadzić core.img podczas używania systemu plików %(fsType)s na "
"%(deviceType)s"

